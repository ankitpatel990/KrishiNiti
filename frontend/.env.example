# FarmHelp Frontend Environment Variables
# Copy this file to .env and update the values as needed.

# Backend API base URL
VITE_API_BASE_URL=http://127.0.0.1:8000

# TensorFlow.js model path (relative to public directory)
VITE_MODEL_PATH=/models

# Application metadata
VITE_APP_NAME=FarmHelp
VITE_APP_VERSION=1.0.0

# Feature flags
VITE_ENABLE_VOICE=true
VITE_ENABLE_OFFLINE=false

# Cache durations
VITE_WEATHER_CACHE_DURATION_MS=300000
VITE_APMC_CACHE_DURATION_MS=600000
